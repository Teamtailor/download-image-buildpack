#!/usr/bin/env bash

# fail fast
set -ueo pipefail

# debug
# set -x

BUILD_DIR=$1
CACHE_DIR=$2
ENV_DIR=$3

DOCKER_REGISTRY=$(cat $ENV_DIR/DOCKER_REGISTRY)

echo "---------BUILD_DIR:"
ls -lha $BUILD_DIR
echo "--------- CACHE_DIR:"
ls -lha $CACHE_DIR
echo "--------- ENV_DIR:"
ls -lha $ENV_DIR
echo "--------- DOCKER_REGISTRY:"
echo $DOCKER_REGISTRY
echo "--------- SOURCE_VERSION:"
echo $SOURCE_VERSION
echo "---------"
echo "all env variables:"
env
echo "---------"
